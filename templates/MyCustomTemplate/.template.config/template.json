{
    "$schema": "http://json.schemastore.org/template",
    "author": "Tymur Krasnianskiy",
    "classifications": [ "Console App" ],
    "identity": "MyCustomTemplate",
    "name": "My custom console template",
    "desciption": "Custom C# .Net Core Template",
    "shortName": "krasnianskiyconsole",
    "sourceName": "MyCustomTemplate",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "symbols": 
    {
      "Framework":{
        "type": "parameter",
        "description": "The target framework for the project",
        "datatype":"choice",
        "choices": [
          {
            "choice": "net6.0"
          },
          {
            "choice": "net5.0"
          }
        ],
        "defaultValue": "net5.0",
        "replaces": "{TargetFramework}"
      }
    },
    "preferNameDirectory": true
}